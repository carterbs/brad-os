{
  "functions": {
    "source": "packages/functions",
    "runtime": "nodejs22",
    "codebase": "default",
    "predeploy": [
      "npm run build -w @brad-os/functions"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5002
    },
    "ui": {
      "enabled": true,
      "port": 4000
    },
    "singleProjectMode": true,
    "export_on_exit": "emulator-data",
    "import": "emulator-data"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
